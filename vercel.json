{
  "builds": [
    {
      "src": "server/node_runner.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**"]
      }
    }
  ],
  "buildCommand": "npm i bun && bun run vercel:prod",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/node_runner.js",
      "methods": ["GET", "POST", "PATCH", "PUT", "DELETE", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
